<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="stock_picking_form_loc_ve" model="ir.ui.view">
            <field name="name">stock.picking.form.loc_ve</field>
            <field name="model">stock.picking</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/group/group/field[@name='min_date']" position="after">
                    <field name="nro_ctrl" />
                </xpath>
            </field>
        </record>
        <record id="stock_picking_out_form_loc_ve" model="ir.ui.view">
            <field name="name">stock.picking.out.form.loc_ve</field>
            <field name="model">stock.picking</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="stock.view_picking_out_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/group/group/field[@name='origin']" position="after">
                    <field name="nro_ctrl" />
                </xpath>
            </field>
        </record>
        <record id="stock_picking_in_form_loc_ve" model="ir.ui.view">
            <field name="name">stock.picking.in.form.loc_ve</field>
            <field name="model">stock.picking</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="stock.view_picking_in_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/group/group/field[@name='backorder_id']" position="after">
                    <field name="nro_ctrl" />
                </xpath>
            </field>
        </record>
    </data>
</openerp>
